{
  "name": "finster",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://nojoystick.github.io/finster",
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "5.1.2",
    "react-scripts": "5.0.0",
